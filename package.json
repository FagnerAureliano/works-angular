    "tslib": "^2.1.0",
    "zone.js": "~0.11.4",
    "@angular/cdk": "^11.2.2",
    "@ngx-translate/core": "^13.0.0",
    "keycloak-angular": "^8.0.1",
    "keycloak-js": "^10.0.2",
    "moment": "^2.29.1",
    "ngx-currency": "^2.5.2",
